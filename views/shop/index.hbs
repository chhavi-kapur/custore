{{# each products }}
  <div class = "row">
    {{# each this }}
        <div class= "col-sm-6 col-md-4 boxy">
          <div class= "thumbnail">
            <img src= "{{ this.imagePath }}" alt= "..." class= "img-responsive">
            <div class= "caption">
              <h3>{{ this.title }}</h3>
              <p class="description">{{ this.description }}</p>
              <div class="clearfix">
                <div class="price pull-left" style="float:left">${{ this.price }}</div>
                  <div style="float:right">
                    <a href="#" class="btn btn-outline-success" role= "button">Buy</a> 
                    <a href="#" class="btn btn-outline-danger" role="button">Save</a>
                  </div>
              </div>
            </div>
          </div>
        </div>
    {{/each}}
  </div>
{{/each}}